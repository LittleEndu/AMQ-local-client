{
  "name": "amq-local-client",
  "version": "1.0.1",
  "description": "Local client for Anime Music Quiz that supports Discord rich presence",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "amq",
    "anime",
    "music",
    "quiz",
    "discord",
    "rich",
    "presence",
    "rpc"
  ],
  "author": "LittleEndu",
  "license": "MIT",
  "devDependencies": {
    "electron": "^6.0.12"
  },
  "dependencies": {
    "electron-edge-js": "12.4.1",
    "electron-window-state": "^5.0.3",
    "open": "^7.0.0"
  },
  "build": {
    "appId": "com.electron.amq.client",
    "productName": "AMQ local client",
    "copyright": "Copyright Â© 2019 LittleEndu",
    "win": {
      "target": "portable",
      "icon": "favicon.ico",
      "extraFiles": [
        "AnimeMusicQuiz.dll",
        "discord_game_sdk.dll"
      ]
    }
  }
}
